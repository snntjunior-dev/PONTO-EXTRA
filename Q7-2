#include <stdio.h>

    int num1, num2;
    
int main() {
    
    printf("Digite o primeiro número: ");
    scanf("%i", &num1);
    
    printf("Digite o segundo número: ");
    scanf("%i", &num2);
    
    if(num1 > num2){
        printf("Ordem crescente: %i - %i", num2, num1);
    }else{
        printf("Ordem crescente: %i - %i", num1, num2);
    }
    
    return 0;
}
