#include <stdio.h>

    int num1, num2;
    
int main() {
    
    printf("Digite o primeiro número: ");
    scanf("%i", &num1);
    
    printf("Digite o segundo número: ");
    scanf("%i", &num2);
    
    if(num1 > num2){
        printf("O número maior é: %i", num1);
    }else{
        printf("O número maior é: %i", num2);
    }
    
    return 0;
}
